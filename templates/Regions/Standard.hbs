{{#template}}
<div id="#REGION_STATIC_ID#" class="ab-region d-flex flex-row justify-content-between #REGION_CSS_CLASSES#" #REGION_ATTRIBUTES#>
	<div class="ab-flex-left align-self-center mr-2">#PREVIOUS#</div>

	<div class="ab-flex-center w-100">
		<div class="ab-region-header">
			<div class="ab-flex-center">
				<div class="ab-region-title"><i class="#ICON_CSS_CLASSES#"></i>#TITLE#</div>
			</div>
			<div class="ab-flex-right">#EDIT#</div>
		</div>

		<div class="ab-region-buttons ab-flex-justify">
			<div class="ab-flex-left">#COPY#</div>
			<div class="ab-flex-center"></div>
			<div class="ab-flex-right">#EXPAND#</div>
		</div>

		<div class="ab-region-body t-Region-body">#BODY##SUB_REGIONS#</div>

		<div class="ab-region-buttons ab-flex-justify">
			<div class="ab-flex-left">#CLOSE##HELP#</div>
			<div class="ab-flex-center">#CHANGE#</div>
			<div class="ab-flex-right">#DELETE##CREATE##CREATE2#</div>
		</div>
	</div>

	<div class="ab-flex-right align-self-center ml-2">#NEXT#</div>
</div>
{{/template}}
